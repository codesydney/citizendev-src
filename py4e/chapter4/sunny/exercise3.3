score = input("Enter Score: ")

try:
    score_as_float = float(score)
    if score_as_float > 1.0 or score_as_float < 0.0:
        print("Score must be between 0.0 and 1.0")
    elif score_as_float >= 0.9:
        print("A")
    elif score_as_float >= 0.8:
        print("B")
    elif score_as_float >= 0.7:
        print("C")
    elif score_as_float >= 0.6:
        print("D")
    else:
        print("F")
except:
    print("Did not enter a valid number")
